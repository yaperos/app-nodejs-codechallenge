  },
  "homepage": "https://github.com/camposgonzalo/app-nodejs-codechallenge#readme",
  "devDependencies": {
    "@serverless/compose": "^1.3.0",
    "serverless": "^3.27.0",
    "serverless-latest-layer-version": "^2.1.2",
    "serverless-offline": "^12.0.4"
  }
}
