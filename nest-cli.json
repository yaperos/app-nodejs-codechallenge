{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/ms-transaction/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/ms-transaction/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/ms-transaction",
  "projects": {
    "ms-antifraud": {
      "type": "application",
      "root": "apps/ms-antifraud",
      "entryFile": "main",
      "sourceRoot": "apps/ms-antifraud/src",
      "compilerOptions": {
        "tsConfigPath": "apps/ms-antifraud/tsconfig.app.json"
      }
    },
    "ms-transaction": {
      "type": "application",
      "root": "apps/ms-transaction",
      "entryFile": "main",
      "sourceRoot": "apps/ms-transaction/src",
      "compilerOptions": {
        "tsConfigPath": "apps/ms-transaction/tsconfig.app.json"
      }
    },
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    }
  }
}