{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/app-nodejs-codechallenge/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/transaction/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/transaction",
  "projects": {
    "transaction": {
      "type": "application",
      "root": "apps/transaction",
      "entryFile": "main",
      "sourceRoot": "apps/transaction/src",
      "compilerOptions": {
        "tsConfigPath": "apps/transaction/tsconfig.app.json"
      }
    },
    "antifraud": {
      "type": "application",
      "root": "apps/antifraud",
      "entryFile": "main",
      "sourceRoot": "apps/antifraud/src",
      "compilerOptions": {
        "tsConfigPath": "apps/antifraud/tsconfig.app.json"
      }
    }
  }
}